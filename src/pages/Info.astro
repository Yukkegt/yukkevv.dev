---
import { db, Link } from 'astro:db';
import Layout from '../layouts/Layout.astro';
import SocialAccounts from '../components/SocialAccounts.astro';

const links = await db.select().from(Link);
---

<Layout>
  {links.map(({ text, url }) => 
    <SocialAccounts service_name={text} url={url} />
  )}
</Layout>